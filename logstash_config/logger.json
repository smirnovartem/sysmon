{
        "template": "map_log_index",
        "mappings": {
                "sysmon": {
                        "properties": {
                                "@timestamp": {
                                        "type": "date",
                                        "format": "strict_date_optional_time||epoch_millis"
                                },
                                "@version": {
                                        "type": "string"
                                },
                                "cpu": {
                                        "type": "double"
                                },
                                "memusage": {
                                        "type": "double"
                                },
                                "memusagesize": {
                                        "type": "double"
                                },
                                "totalmemsize": {
                                        "type": "double"
                                },
                                "freememsize": {
                                        "type": "double"
                                }
                        }
                }
        }
}